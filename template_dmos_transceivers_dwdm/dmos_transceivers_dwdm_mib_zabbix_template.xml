            <template>DM Template - DmOS Transceivers DWDM</template>
            <name>DM Template - DmOS Transceivers DWDM</name>
            <groups>
                <group>
                    <name>DM - Templates</name>
                </group>
            </groups>
            <discovery_rules>
                <discovery_rule>
                    <uuid>22a98a0ef95b4eea898646d77f0949f8</uuid>
                    <name>DmOS Transceivers DWDM</name>
                    <type>EXTERNAL</type>
                    <key>dmos_dwdm_mib.py[&quot;{HOST.CONN}&quot;, &quot;{$SNMP_COMMUNITY}&quot;]</key>
                    <delay>60m</delay>
                    <lifetime>30d</lifetime>
                    <enabled_lifetime_type>DISABLE_NEVER</enabled_lifetime_type>
                    <item_prototypes>
                        <item_prototype>
                            <uuid>07b648ab8a4a415d84b1d95f8608334b</uuid>
                            <name>DWDM TCV Chromatic Dispersion : Interface {#IFDESCR}({#IFALIAS})</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>get[DMOS-TRANSCEIVER-MIB::chromaticDispersion.{#SNMPINDEX}]</snmp_oid>
                            <key>transceiverDwdmChromaticDispersion.[{#SNMPINDEX}]</key>
                            <delay>5m</delay>
                            <units>ps/nm</units>
                            <description>Current transceiver Chromatic Dispersion</description>
                            <tags>
                                <tag>
                                    <tag>Application</tag>
                                    <value>Transceivers DWDM Diagnostics</value>
                                </tag>
                            </tags>
                        </item_prototype>
                        <item_prototype>
                            <uuid>f2d9a56812ee42068c3a83f87d596040</uuid>
                            <name>DWDM TCV Diagnostics FERC : Interface {#IFDESCR}({#IFALIAS})</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>get[DMOS-TRANSCEIVER-MIB::ferc.{#SNMPINDEX}]</snmp_oid>
                            <key>transceiverDwdmDiagFerc.[{#SNMPINDEX}]</key>
                            <delay>5m</delay>
                            <value_type>TEXT</value_type>
                            <description>Current transceiver FERC</description>
                            <tags>
                                <tag>
                                    <tag>Application</tag>
                                    <value>Transceivers DWDM Diagnostics</value>
                                </tag>
                            </tags>
                        </item_prototype>
                        <item_prototype>
                            <uuid>804884c589fa4e7582c41bff23942039</uuid>
                            <name>DWDM TCV Diagnostics OSNR : Interface {#IFDESCR}({#IFALIAS})</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>get[DMOS-TRANSCEIVER-MIB::osnr.{#SNMPINDEX}]</snmp_oid>
                            <key>transceiverDwdmDiagOsnr.[{#SNMPINDEX}]</key>
                            <delay>5m</delay>
                            <value_type>FLOAT</value_type>
                            <units>dB</units>
                            <description>Current transceiver OSNR</description>
                            <preprocessing>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <parameters>
                                        <parameter>0.01</parameter>
                                    </parameters>
                                </step>
                            </preprocessing>
                            <tags>
                                <tag>
                                    <tag>Application</tag>
                                    <value>Transceivers DWDM Diagnostics</value>
                                </tag>
                            </tags>
