<zabbix_export>
    <version>7.2</version>
    <template_groups>
        <template_group>
            <uuid>41aa33b09cbf4e769891dd9db0b11ba8</uuid>
            <name>DATACOM</name>
        </template_group>
        <template_group>
            <uuid>85dcfb92cf3f48e7bb9089342e98559c</uuid>
            <name>DM - Templates</name>
        </template_group>
