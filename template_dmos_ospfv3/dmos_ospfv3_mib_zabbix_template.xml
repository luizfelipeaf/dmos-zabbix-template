                            </tags>
                        </item_prototype>
                        <item_prototype>
                            <uuid>844252419feb404797efaf297f0929d4</uuid>
                            <name>OSPFv3 External LSAs Counter</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>get[OSPFV3-MIB::ospfv3ExtLsaCount.{#SNMPINDEX}]</snmp_oid>
                            <key>ospfv3.external.lsa.count.[{#SNMPINDEX}]</key>
                            <delay>10m</delay>
                            <tags>
                                <tag>
                                    <tag>Application</tag>
                                    <value>OSPFv3 Status</value>
                                </tag>
                            </tags>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
            </discovery_rules>
